{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Sid": "AllowDeleteObject",
          "Effect": "Allow",
          "Principal": {
              "AWS": "<my-user-arn>"
          },
          "Action": [
              "s3:DeleteObject"
          ],
          "Resource": [
              "<my-bucket-arn>/*"
          ]
      }
  ]
}